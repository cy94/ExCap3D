ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/remote/cuda-11.7/bin/nvcc

cflags = -pthread -B /rhome/cyeshwanth/miniconda3/envs/mask3d_2/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/TH -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/THC -I/usr/local/remote/cuda-11.7/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_cuda_utils -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/TH -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/lib/python3.7/site-packages/torch/include/THC -I/usr/local/remote/cuda-11.7/include -I/rhome/cyeshwanth/miniconda3/envs/mask3d_2/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_cuda_utils -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





